{{> navUser}}
 
 
<script>
  $(window).load(function () {
    $('#myModal').modal('show');
  });
</script>

<script>
  function sure(id,precioI,precio) {
    document.getElementById("idmueble").value = id;
    document.getElementById("preciototal").value = precio;
    document.getElementById("usertotal").value = precio;
    $('#compra').modal('show');
  }
</script>

<script>
   function validateForm(){
      
      if(document.getElementById("envio").checked==false &&
         document.getElementById("tienda").checked==false &&
        document.getElementById('instalacion').checked==false
      ){
          return false;
      }

      if(document.getElementById("envio").checked==true ||
         document.getElementById("tienda").checked==true ||
        document.getElementById('instalacion').checked==true){
          return true;
      }

  }
</script>

<script>
   function exe(){
    const cb = document.getElementById('instalacion');
    const copiaPrecio= parseInt(document.getElementById('preciototal').value);
    var variable = document.getElementById('usertotal');
    var precionormal =parseInt(variable.value);
    var instalacion = 20000+70000;
    var precio=precionormal+instalacion;
    if(cb.checked){
      text.style.display = "block";
      document.getElementById("envio").disabled = true;
      document.getElementById("tienda").disabled = true;
      document.getElementById("usertotal").value=precio;
    }
    if(!cb.checked){
      text.style.display = "none";
       document.getElementById("envio").disabled = false;
       document.getElementById("tienda").disabled = false;
        document.getElementById("usertotal").value=copiaPrecio;
    }
   };
</script>

<script>
function env(){
const copiaPrecio= parseInt(document.getElementById('preciototal').value);
const cb = document.getElementById('envio');
var envio = 20000;
 if(cb.checked){
  document.getElementById("tienda").disabled = true;
  document.getElementById('instalacion').disabled=true;
  document.getElementById("usertotal").value=copiaPrecio+envio;
 }
if(!cb.checked){
 document.getElementById("tienda").disabled = false;
  document.getElementById('instalacion').disabled=false;
   document.getElementById("usertotal").value=copiaPrecio;
}
};
</script>

<script>
function tiend(){
const copiaPrecio= parseInt(document.getElementById('preciototal').value);
const cb = document.getElementById('tienda');
var envio = 20000;
 if(cb.checked){
  document.getElementById("envio").disabled = true;
  document.getElementById('instalacion').disabled=true;
  document.getElementById("usertotal").value=copiaPrecio;
 }
if(!cb.checked){
 document.getElementById("envio").disabled = false;
  document.getElementById('instalacion').disabled=false;
   document.getElementById("usertotal").value=copiaPrecio;
}
};
</script>



<div class="modal fade" id="myModal" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header" style="background-color: rgb(67, 179, 160);">

        <h4 class="modal-title"><i class="fas fa-couch"></i>&nbsp; Bienvenido a prontoMueble S.A</h4>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
      </div>
      <div style="background-color: rgb(239, 245, 243);">
        <div class="modal-body">
          <h6>Compra muebles de todo tipo color y dimensiones a tu gusto!</h6>
        </div>
        <div align='center'>
          <img
            src="data:image/jpeg;base64,">
        </div>
      </div>
      <div class="modal-footer"></div>

    </div>

  </div>
</div>

<div class="modal fade" id="compra" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header" style="background-color: rgb(67, 179, 160);">
        <h4 class="modal-title"><i class="fas fa-shopping-cart"></i>&nbsp; Confirmar datos de compra</h4>
      </div>
       <h7 id="text" style="display:none;background-color:rgb(231, 231, 231);color:rgb(39, 36, 36);text-align: center">Nota: se incluiran gastos de envio</h7>
     <hr>
      <div class="modal-body">
        <form action="/links/comprar" onsubmit="return validateForm()" method="POST">
        <div class="form-check">
  <input class="form-check-input" type="checkbox" value=""  id="instalacion" onclick="exe();">
  <label class="form-check-label" for="instalacion"> Incluir  instalacion&nbsp; <i class="fas fa-toolbox"></i><hr></label>
</div>
<div class="form-check">
  <input class="form-check-input" type="checkbox" value="" id="tienda" onclick="tiend();">
  <label class="form-check-label" for="tienda">
    Recoger en tienda&nbsp; <i class="fas fa-running"></i>
    <hr>
  </label>
</div>

<div class="form-check">
  <input class="form-check-input" type="checkbox" value="" id="envio" onclick="env();">
  <label class="form-check-label" for="envio">
    Incluir  Envio &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <i class="fas fa-truck"></i>
    <hr>
  </label>
</div>

<input type="hidden" name="id_mueble" id="idmueble" value="">

<div>
   <input type="hidden" id="preciototal" value=""> </span>
 Total &nbsp; :&nbsp; <i class="fas fa-dollar-sign"></i> &nbsp;<input type="number" name="valorCompra" id="usertotal" value="" style="border: 0;" > </span>
 <hr>
</div>
          <button type="submit" class="btn btn-success btn-block"> <i class="fas fa-check-circle"></i>&nbsp;confirmar compra</button>
        </form>
      </div>
    </div>
  </div>
</div>

<div class="container ">
  <div class="row">
    {{#each muebles }}
    <div class="col-md-4">
      <div class="panel panel-default pt-4">
        <div class="card">
          <div class="card-header">
            <h6><i class="fas fa-map-marker"></i></h6>
            <h2 class="text-center" style="text-transform: capitalize;">{{tipo}} de {{material}}, {{color}}</h2>
          </div>
          <div class="card-body justify-content-center">
            <div class="panel-heading ">
            </div>
            <div class="panel-body">
              <h5>Proveedor: {{nombre_prov}}</h5>
              <h5>Vendedor: {{nombre_vend}}</h5>
              <hr>
              <h5>Precio:</h5>
                <h5></h5><i class="fas fa-dollar-sign"></i> {{precio}}</h5>
            </div>
          </div>
          <div class="card-footer">
            <a onclick="sure({{id}},{{precio_instalacion}},{{precio}});" class="btn btn-success btn-block"><i
                class="fas fa-shopping-cart"></i>&nbsp;
              Comprar</a>
          </div>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>