{{> navUser}}

<script>
  $(window).load(function () {
    $('#myModal').modal('show');
  });
</script>

 <script>
    function sure(id) {
       $(".open-AddBookDialog").click(function(){
     $('#bookId').val($(this).data('id'));
    $("#addBookDialog").modal("show");
  });
    }
</script>

<div class="modal fade" id="myModal" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header" style="background-color: rgb(67, 179, 160);">

        <h4 class="modal-title"><i class="fas fa-couch"></i>&nbsp; Bienvenido a prontoMueble S.A</h4>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
      </div>
      <div style="background-color: rgb(239, 245, 243);">
        <div class="modal-body">
          <h6>Compra muebles de todo tipo color y dimensiones a tu gusto!</h6>
        </div>
        <div align='center'>
          <img
            src="data:image/jpeg;base64,">
          <!-- <button type="button" class="btn btn-success btn-block" data-dismiss="modal"></button>-->
        </div>
      </div>
      <div class="modal-footer"></div>

    </div>

  </div>
</div>

<div class="modal fade" id="compra" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header" style="background-color: rgb(67, 179, 160);">

        <h4 class="modal-title"><i class="fas fa-shopping-cart"></i>&nbsp; Confirmar datos de compra</h4>
      </div>
      
       <div class="modal-body">
  <form action="/prontomueblesa/comprar" method="GET">
     
<input id="prodId" name="prodId" type="hidden" value="">


 <button type="submit" class="btn btn-success btn-block" > confirmar compra</button>
    </form>
    </div>
</div>
  </div>
</div>




<div class="container ">
  <div class="row">
    {{#each muebles }}
    <div class="col-md-4">
      <div class="panel panel-default pt-4">
        <div class="card">
          <div class="card-header">
            <h2 class="text-center" style="text-transform: capitalize;">{{tipo}} de {{material}}, {{color}}</h2>
          </div>
          <div class="card-body justify-content-center">
            <div class="panel-heading ">


            </div>
            <div class="panel-body">
              <h5>Proveedor: {{nombre_prov}}</h5>
              <h5>Vendedor: {{nombre_vend}}</h5>
              <hr>
              <h5>Precio: {{precio}}</h5>
            </div>
          </div>
          <div class="card-footer">
          <a onclick="return sure({{id}});" class="btn btn-success btn-block" ><i class="fas fa-shopping-cart"></i>&nbsp;
            Comprar</a>
        </div>
        </div>
        
      </div>
    </div>

    {{/each}}
  </div>
</div>