<div class="container pt-4">
    <div class="row justify-content-center">
        <div class="col-md- col-sm-5">
            <div class="card">
                <div class="card-header">
                    <h1>Registrar mueble</h1>
                </div>
                <form action="/#">
                <div class="card-body">
                    <div class="form-group">
                        <input class="form-control"  type="text" name="nombrepro" placeholder="Nombre del precio">
                    </div>
                    <div class="form-group">
                        <input class="form-control"  type="text" name="nombrepro" placeholder="Dimensiones (alto, ancho, profundo)">
                    </div>
                    <div class="form-group">
                        <input class="form-control"  type="number" name="nombrepro" placeholder="Precio de instalacion">
                    </div>
                    <div class="form-group">
                        <p>Material: 
                        <select name="material" id="material" class="form-control">
                        {{#each material}}
                            <option >{{material}}, {{id}}</option>
                        {{/each}}
                        </select>
                    </div>
                    <div class="form-group">
                        <p>Color: 
                        <select name="color" id="color" class="form-control">
                        {{#each color}}
                            <option >{{color}}, {{id}}</option>
                        {{/each}}
                        </select>
                    </div>
                    <div class="form-group">
                        <p>Tipo: 
                        <select name="tipo" id="tipo" class="form-control">
                        {{#each tipo}}
                            <option >{{tipo}}, {{id}}</option>
                        {{/each}}
                        </select>
                    </div>
                    <div class="form-group">
                        <p>Vendedor: 
                        <select name="tipo" id="tipo" class="form-control">
                        {{#each vendedor}}
                            <option >{{id}}</option>
                        {{/each}}
                        </select>
                    </div>
                    <div class="form-group">
                        <p>Tipo: 
                        <select name="tipo" id="tipo" class="form-control">
                        {{#each proveedor}}
                            <option >{{nombre}}, {{id}}</option>
                        {{/each}}
                        </select>
                    </div>
                    <div class="form-group">
                        <button class="btn btn-success btn-block" type="submit">Registrar</button>
                    </div>
                </div>
                </form>
            </div>
        </div>
    </div>
</div>

