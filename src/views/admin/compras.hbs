{{> navAdmin}}
<div class="container ">
  <div class="row">
    {{#each compras}}
    <div class="col-md-4">
      <div class="panel panel-default pt-4">
        <div class="card">
          <div class="card-header">
            <h6><i class="fas fa-map-marker"></i></h6>
            <h2 class="text-center" style="text-transform: capitalize;">{{tipo}} de {{material}}, {{color}}</h2>
          </div>
          <div class="card-body justify-content-center">
            <div class="panel-heading ">
            </div>
            <div class="panel-body">
              <h5>Fecha: {{fecha}}</h5>
            </div>
          </div>
          <div class="card-footer">
            <h5>Id Clinte: {{id_cliente}}</h5>
          </div>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>